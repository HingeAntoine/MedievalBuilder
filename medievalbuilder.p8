 for i=1,3 do
  local sprjob= jobs[jobs_drawn[i]].jobspr
  spr(sprjob,xjobs[i],85)
 end
end

function draw_quests()
 local xjobs={67,99}

 for i=4,5 do
  local qtext= jobs[jobs_drawn[i]].text
  for j=1,#qtext do
   print(qtext[j],xjobs[i-3],85+(j-#qtext)*6,5)
  end
 end
 
