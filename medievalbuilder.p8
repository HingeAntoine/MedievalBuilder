pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- micro medieval deck building game


--max # of cards should be 5/6?

--game variables
p1={
 meep=0,
 deck={},
 discard={}
}

--[[p2, p3, p4...]]

turn_start=true
zone_selected=1

meep_center=30
coins=0

cards_hand={}
cards_disc={}
playfield_stashes={}

curr_card=1
curr_playfield=0
curr_player=1

nstash=6

--job stashes
job_banker=1
job_librar=2
job_wizard=3
job_priest=4
job_witch =5
job_psychi=6
job_mortic=7

