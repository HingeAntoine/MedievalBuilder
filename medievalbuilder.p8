  	inf=false},
 	low_card={
  	card=create_card(42,{{constdraw,1}}),
  	price=2,
  	inf=false}
	}
	
	local wizard={
	 jobspr=18,
 	top_card={
  	card=create_card(34,{{constdraw,3}}),
  	price=6,
